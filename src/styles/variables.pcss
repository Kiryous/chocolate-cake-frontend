/* Переменные */
:root {
    --bold: "Gilroy-ExtraBold", Arial, sans-serif;
    --light: "Gilroy-Light", Arial, sans-serif;
    --link: rgb(25, 25, 25);
    --hover: rgb(56, 56, 56);
    --visited: rgb(3, 9, 4);
    --pink: #f94084;
}


@custom-media --latex-tablet (width <= 1000px);
@custom-media --latex-mobile (width <= 640px);
